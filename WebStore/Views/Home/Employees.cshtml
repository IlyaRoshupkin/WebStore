@model IEnumerable<WebStore.Models.Employee>


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Employees</title>
</head>
<body>
    <h3>Employees</h3>
    <table>
        <thead>
            <tr>
                <th>id</th>
                <th>LastName</th>
                <th>FirstName</th>
                <th>Patronymic</th>
                <th>Age</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var employee in Model)
            {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.LastName</td>
                <td>@employee.FirstName</td>
                <td>@employee.Patronymic</td>
                <td>@employee.Age</td>
                <td>@Html.ActionLink("Детали", "Details", new { id = employee.Id })</td>
            </tr>
            }
        </tbody>
    </table>
    <hr />
    <a href="~/">Home</a>
</body>
</html>
